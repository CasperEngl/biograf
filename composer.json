{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "alexbowers/nova-prepopulate-searchable": "^1.0",
        "caouecs/laravel-lang": "~4.0",
        "devant/cinema-maker": "*",
        "ebess/advanced-nova-media-library": "^2.9",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.0",
        "laravel/horizon": "^3.4",
        "laravel/nova": "^2.3",
        "laravel/tinker": "^1.0",
        "laravel/vapor-core": "^2.0",
        "laravolt/avatar": "^3.0",
        "league/color-extractor": "^0.3.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "livewire/livewire": "^0.3.7",
        "nothingworks/blade-svg": "^0.3.2",
        "php-tmdb/api": "^2.1",
        "predis/predis": "^1.1",
        "spatie/image": "^1.7",
        "spatie/laravel-blade-x": "^2.3",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-newsletter": "^4.7",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-sluggable": "^2.2",
        "spatie/laravel-translatable": "^4.2",
        "spatie/laravel-view-models": "^1.2",
        "spatie/nova-translatable": "^2.0",
        "tightenco/ziggy": "^0.8.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel-frontend-presets/tailwindcss": "^2.0",
        "laravel/ui": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "slevomat/coding-standard": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "./app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/CinemaMaker"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish"
        ]
    }
}
